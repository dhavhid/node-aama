<!-- Event Listing -->
<section class="businesses-section">
    <div class="container margin-tb-lg">
        <div class="row">
            <div class="col-md-12">
                <div class="business-entry">
                    <ol class="breadcrumb">
                        <li><a href="/"><i class="fa fa-home"></i></a></li>
                        <li class="active">{{title}}</li>
                    </ol>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-8 col-sm-8 col-xs-12">
                <input type="text" name="keyword" disabled="disabled" placeholder="Keyword Search" id="keyword" class="form-control" onkeyup="window.Cubic.labs.searchKeyUp(this.value, event);" onkeypress="window.Cubic.labs.searchLabs(this.value, event);">
                <span class="help-block">Type your criteria and then hit Enter.</span>
            </div>
            <div class="col-md-4 col-sm-4 col-xs-12">
                <select name="basedon" disabled="disabled" id="basedon" class="form-control" onchange="window.Cubic.labs.getLabs();">
                    {{#each states}}
                        {{#if this}}
                            <option value="{{this}}">{{this}}</option>
                        {{else}}
                            <option value="">Select State or Province</option>
                        {{/if}}
                    {{/each}}
                </select>
            </div>
        </div>
        <hr />
        <div class="row">
            <div class="col-md-4 col-sm-5 col-xs-12 pages-info"></div>
            <div class="col-md-8 col-sm-7 col-xs-12 pages text-right"></div>
        </div>
        <div class="row" id="business-container"></div>
        <div class="row">
            <div class="col-md-12 pages text-right"></div>
        </div>
    </div> <!-- End container -->
</section>
<!-- End of Businesses Listing -->
<script type="text/javascript">
    $(function(){
        window.Cubic.labs.getLabs();
    });
</script>