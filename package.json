{
  "name": "aama-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "js",
  "scripts": {
    "start": "node --harmony server",
    "start:dev": "nodemon --debug=5959 --harmony -e js,hbs server",
    "build:js": "browserify ./js -d -g [uglifyify -x .js] | exorcist static/assets/build/bundle.js.map > static/assets/build/bundle.js",
    "build:sass": "node-sass sass/index.scss static/assets/build/bundle.css",
    "build:css-prefixes": "postcss --use autoprefixer --autoprefixer.browsers '> 1%' --replace static/assets/build/bundle.css",
    "watch:js": "watchify ./js -d -o static/assets/build/bundle.js",
    "watch:css": "watch 'npm run build:sass' ./sass",
    "watch:inspector": "node-inspector",
    "watch": "parallelshell 'npm run watch:css' 'npm run watch:js'",
    "build": "npm run build:sass && npm run build:css-prefixes && npm run build:js",
    "dev": "parallelshell 'npm run watch:inspector' 'npm run watch:js' 'npm run watch:css' 'sleep 2 && npm run start:dev'"
  },
  "author": "",
  "license": "none",
  "devDependencies": {
    "autoprefixer": "^5.1.0",
    "bootstrap": "^3.3.4",
    "exorcist": "^0.4.0",
    "node-sass": "^3.0.0-alpha.0",
    "postcss-cli": "^1.0.0",
    "uglifyify": "^3.0.1",
    "watch": "^0.14.0",
    "watchify": "^2.6.0"
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "bootpag": "^1.0.7",
    "chosen-npm": "^1.4.2",
    "connect-mongo": "^0.8.2",
    "cookie": "^0.2.3",
    "cookies": "^0.5.1",
    "crypto-js": "^3.1.6",
    "dateformat": "^1.0.11",
    "dotenv": "^1.2.0",
    "easysoap": "file:vendor/easysoap",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "express-session": "^1.12.1",
    "jquery.inputmask": "^3.3.0",
    "local-storage": "^1.4.2",
    "lodash": "^3.10.1",
    "masonry-layout": "^4.0.0",
    "moment": "^2.10.6",
    "numeral": "^1.5.3",
    "query-string": "^3.0.0",
    "soap": "^0.11.0",
    "striptags": "^2.0.4",
    "superagent": "^1.4.0"
  }
}
